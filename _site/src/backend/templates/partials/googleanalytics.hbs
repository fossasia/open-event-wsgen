<!-- Global site tag (gtag.js) - Google Analytics -->

{{#if ganalyticsID}}
  <script async src="https://www.googletagmanager.com/gtag/js?id={{ganalyticsID.id}}"></script>
  <script>
    window.dataLayer = window.dataLayer || [];
    function gtag(){dataLayer.push(arguments);}
    gtag('js', new Date());

    gtag('config', '{{ ganalyticsID.id}}');
  </script>
{{/if}}
