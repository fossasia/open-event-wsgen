                $(this).show();
              }
              else {
                $(this).hide();
              }
            });

            if (timeFlag) {
              dayFlag = true;
              $(this).show();
            }
            else {
              $(this).hide();
            }
          });

          if (dayFlag) {
            $(this).show();
          }
          else {
            $(this).hide();
          }
