<input type="text" data-format="hex" class="form-control" id="cp8" value="primary"/>
<script>
    $(function () {
        $('#cp8').colorpicker({
            colorSelectors: {
                'default': '#777777',
                'primary': '#337ab7',
                'success': '#5cb85c',
                'info': '#5bc0de',
                'warning': '#f0ad4e',
                'danger': '#d9534f'
            }
        });
    });
</script>
